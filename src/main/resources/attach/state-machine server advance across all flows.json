            "method": "PATCH",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "url": "{{baseUrl}}/process/{{processId}}/variables",
            "body": { "mode": "raw", "raw": "{ \"voiceScore\": 0.97 }" }
          }
        }
      ]
    },
